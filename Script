    Name = "Spam Trap Murder",
    Default = false,
    Callback = function(Value)
        getgenv().SpamTrapMurder = Value
        while SpamTrapMurder do task.wait(1.5)
        pcall(function()
for _,v in pairs(game.Players:GetPlayers()) do
if v.Character ~= nil and v.Backpack:FindFirstChild("Knife") or v.Character:FindFirstChild("Knife") then
game.ReplicatedStorage.TrapSystem.PlaceTrap:InvokeServer(v.Character.HumanoidRootPart.CFrame * CFrame.new(0, -1, 0))
end
end
end)    
        if not SpamTrapMurder then
        break
        end
    end
end
})

