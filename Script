 Name = "TrapPlayer",
 Default = "",
 TextDisappear = false,
 Callback = function(txt)
 getgenv().Target = txt
 end   
})

Tab:AddToggle({
 Name = "Spam Trap Player",
 Default = false,
 Callback = function(Value)
         getgenv().SpamTrapTarget = Value
         while SpamTrapTarget do task.wait(1.5)
         pcall(function()
         local Target = gplr(getgenv().Target)
game:GetService("ReplicatedStorage"):WaitForChild("TrapSystem"):WaitForChild("PlaceTrap"):InvokeServer(game.Players[Target].Character.HumanoidRootPart.CFrame)
end)
        if not SpamTrapTarget then
        break
        end
    end
end
})



Tab:AddToggle({
    Name = "Spam Trap Players",
    Default = false,
    Callback = function(Value)
        getgenv().SpamTrap = Value
        while SpamTrap do task.wait(1.5)
        pcall(function()
for _, v in pairs(game.Players:GetChildren()) do
if v ~= game.Players.LocalPlayer then
if v.Character then
if v.Character:FindFirstChild("HumanoidRootPart") then
game.ReplicatedStorage.TrapSystem.PlaceTrap:InvokeServer(v.Character.HumanoidRootPart.CFrame * CFrame.new(0, -1, 0))
end
end
end
end
end)    
        if not SpamTrap then
        break
        end
    end
end
})

