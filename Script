Tab:AddButton({
    Name = "More Guns 2",
    Callback = function()
plr = game.Players.LocalPlayer
toolc = plr.Backpack:GetChildren()
x = -20
y = -5
z = -20
for _, v in toolc do
 if v:IsA("Tool") then
  if v:FindFirstChild("Handle") then
   v.Parent = plr.Character
   v.Grip = CFrame.new(x, y, z)
   v.Parent = plr.Backpack
   v.Parent = plr.Character
   if x ~= 20 then
    x += 4
   elseif x == 20 then
    if z ~= 20 then
     x = -20
     z += 4
    elseif z == 20 then
     x = -20
     z = -20
     y += 4
    end
   end
  end
 end
end
end
})

Tab:AddButton({
    Name = "Cube",
    Callback = function()
plr = game.Players.LocalPlayer
toolc = plr.Backpack:GetChildren()
x = -5
y = -5
z = -5
for _, v in toolc do
 if v:IsA("Tool") then
  if v:FindFirstChild("Handle") then
   v.Parent = plr.Character
   v.Grip = CFrame.new(x, y, z)
   v.Parent = plr.Backpack
   v.Parent = plr.Character
   if x ~= 5 then
    x += 2
   elseif x == 5 then
    if z ~= 5 then
     x = -5
     z += 2
    elseif z == 5 then
     x = -5
     z = -5
     y += 2
    end
   end
  end
 end
 end
end
})



