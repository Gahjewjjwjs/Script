Tab:AddToggle({
 Name = "Break Gun",
 Default = false,
 Callback = function(Value)
 breakgunl = false
 function FindGun(v)
 if v.Character then
  local a,b = v.Backpack:FindFirstChild("Gun"), v.Character:FindFirstChild("Gun")
  if a then
   return a
  elseif b then
   return b
  end
  if not a and not b then
   return false
  end
 end
end
if Value then
  if breakgunl == false then
   breakgunl = true
   while breakgunl do
    for _, v in game.Players:GetChildren() do
     if v ~= game.Players.LocalPlayer then
      if FindGun(v) then
       FindGun(v).KnifeServer.ShootGun:InvokeServer(1, 0, "AH")
      end
     end
    end
    wait(0.4)
   end
   end
  else
   breakgunl = false
  end
 end
})

Tab:AddToggle({
    Name = "Invisible",
    Default = false,
    Callback = function(Value)
            if Value then
        local args = {
    [1] = true
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Gameplay"):WaitForChild("Stealth"):FireServer(unpack(args))

    else
        local args = {
    [1] = false
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Gameplay"):WaitForChild("Stealth"):FireServer(unpack(args))

    end
end
})

